mixin sPlan(data)
	// start sPlan
	+b.SECTION.sPlan.section#sPlan&attributes(attributes)
		.container
			+b.section-title.text-center
				h2 План выставки
			+e.A.img(href="img/@2x/sPlan-img.png" data-fancybox="gallery")
				img(src="img/@2x/sPlan-img.png", alt="")
			button.btn Забронировать стенд
	// end sPlan